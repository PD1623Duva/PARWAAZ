<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>3D Pie Chart</title>
  <script src="https://code.highcharts.com/highcharts.js"></script>
  <script src="https://code.highcharts.com/highcharts-3d.js"></script>
  <script src="https://code.highcharts.com/modules/exporting.js"></script>
  
</head>
<body>
  <div id="container" style="height: 600px; width: 600px; margin: auto;"></div>
  <h1 id="h1">hello</h1>


  <script>
    Highcharts.chart('container', {
      chart: {
        type: 'pie',
        options3d: {
          enabled: true,
          alpha: 45,
          beta: 0
        }
      },
      title: {
        text: 'Your Result'
      },
      plotOptions: {
        pie: {
          allowPointSelect: true,
          cursor: 'pointer',
          depth: 45,
          dataLabels: {
            enabled: true,
            format: '{point.name}'
          }
        }
      },
      series: [{
        name: 'Percentage',
        data: [
          ['Journalism', <%= stats.journ %>],
          ['Medical', <%= stats.med %>],
          ['Bacherlor of Arts', <%= stats.ba %>],
          ['Chatent Acoutant', <%= stats.ca %>],
          ['Law', <%= stats.law %>],
          ['B-Tech CSE', <%= stats.cse %>],
          ['B-Tech Mech', <%= stats.mech %>],
          ['B-Tech Civil', <%= stats.civil %>],
          ['B-Tech ECE', <%= stats.ece %>],
          ['Bacherlor of Computer Application', <%= stats.bca %>],
          ['Bacherlor of Education', <%= stats.bed %>],
          ['Bacherlor of Buissness Administration', <%= stats.bba %>],
          ['Bacherlor of Science', <%= stats.bsc %>],
          ['Bacherlor of Communication', <%= stats.bcom %>]
        ]
      }]
    });
  </script>
  
</body>
</html>
